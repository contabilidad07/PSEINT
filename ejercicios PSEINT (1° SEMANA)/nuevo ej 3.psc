Algoritmo sin_titulo
	definir precio1, precio2, precio3, promedio Como Real;
	Escribir "Ingrese el precio en el mercado 1";
	Leer precio1;
	
	Escribir "Ingrese el precio en el mercado 2";
	Leer precio2;
	
	Escribir "Ingrese el precio en el mercado 3";
	Leer precio3;
	
	promedio = (precio1 + precio2 + precio3)/3;
	
	Escribir "El precio promedio es $", promedio;
	

FinAlgoritmo
